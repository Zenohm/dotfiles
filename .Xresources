! Set the term variable correctly.
XTerm.termName: xterm-256color

! Misc
xterm*vt100*geometry: 90x30

! Set default font and size.
XTerm.vt100.locale: true
XTerm.vt100.renderFont: true
Xterm*font: terminus-19
XTerm.vt100.faceName: Iosevka Term Heavy:size=12:antialias=true
! Fix backspace key.
XTerm.vt100.backarrowKey: false
XTerm.ttyModes: erase ^?
! special
*.foreground:   #c5c8c6
!*.background:   #212121
!*.background:   #2f343f
*.background:   #2f2f2f
*.cursorColor:  #c5c8c6
URxvt.font: xft:RobotoMono Nerd Font:size=13:style=Medium
URxvt.scrollBar: false
Urxvt.secondaryScroll: true
URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true
!URxvt.transparent: true
!URxvt.tintColor: #2d2d2d
!URxvt.shading: 90
!URxvt.blurRadius: 10
URxvt*depth: 32
URxvt*background: rgba:0000/0000/0200/d800

! black
*.color0:       #1d1f21
*.color8:       #969896

! red
*.color1:       #cc342b
*.color9:       #cc342b

! green
*.color2:       #198844
*.color10:      #198844

! yellow
*.color3:       #fba922
*.color11:      #fba922

! blue
*.color4:       #3971ed
*.color12:      #3971ed

! purple
*.color5:       #a36ac7
*.color13:      #a36ac7

! cyan
*.color6:       #3971ed
*.color14:      #3971ed

! white
*.color7:       #c5c8c6
*.color15:      #ffffff

